# Backend Integration
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000

# Letta settings
LETTA_API_KEY=<OPTIONAL_LETTA_TOKEN_OR_PASSWORD>

# Letta Local
LETTA_BASE_URL=http://localhost:8283
# For Letta Cloud, use the value below for your base url instead
# LETTA_BASE_URL=https://lettalettalatest-production-4de4.up.railway.app

# Letta Global API Key for accessing user agents
NEXT_PUBLIC_LETTA_GLOBAL_KEY=your-global-key

# Authentication settings (legacy - now handled by backend integration)
USE_COOKIE_BASED_AUTHENTICATION=false # set to false when using backend auth
NEXT_PUBLIC_CREATE_AGENTS_FROM_UI=false # agents are now created through backend
